tate.global.isLogin)\r\n\r\n      if(path.includes('pdtail')) {\r\n        obj.left = this.list.projectDetail.left\r\n        obj.right = this.list.projectDetail.right\r\n      } else {\r\n        obj.left = this.list.noAccess.left\r\n      }\r\n\r\n      switch(path) {\r\n        case '/':\r\n          obj.right = this.list.noAccess.home\r\n          break\r\n        case '/community':\r\n          obj.right = this.list.noAccess.community\r\n          break\r\n        case '/signin':\r\n          obj.right = this.list.noAccess.sign\r\n          break\r\n        case '/demo':\r\n          obj.right = this.list.noAccess.home\r\n          break\r\n        case '/projects':\r\n          obj.right = this.list.access.right\r\n          break\r\n      }\r\n\r\n      if(this.$store.state.global.isLogin) {\r\n        obj.right = this.list.access.right\r\n      }\r\n\r\n      this.menuList = obj\r\n    }\r\n  },\r\n  \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.nav-container {\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 0 30px;\r\n}\r\n\r\nimg {\r\n  width: 100px;\r\n}\r\n\r\nbutton {\r\n  color: #fff;\r\n}\r\n\r\n.left-menu {\r\n  display: flex;\r\n}\r\n\r\n.route-menu {\r\n  padding: 0 10px;\r\n  color: #999;\r\n  transition: color .2s ease-in;\r\n}\r\n\r\n.route-menu:hover {\r\n  color: #fff;\r\n}\r\n\r\n.check {\r\n  color: #fff;\r\n}\r\n</style>"],"sourceRoot":""}]}